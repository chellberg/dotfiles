# Sample gitconfig
#

[hub]
        protocol = https
[user]
        name = AUTHORNAME
        email = AUTHOREMAIL
[credential]
        helper = GIT_CREDENTIAL_HELPER
[color]
        ui = auto
[push]
        default = simple
[alias]
        a = add
        s = status
        lg = log
        l = pull --rebase
        lgg = log --color --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%C(bold blue) <%an>%Creset"
        lgs = log --stat
        c = commit
        p = push
        pu = push -u
        co = checkout
        cb = checkout -b
        b = branch
        d = diff
        dc = diff --cached
        rhh = reset --hard HEAD
        rpo = remote prune origin

        # list aliases
        la = "!git config -l | grep alias | cut -c 7-"
